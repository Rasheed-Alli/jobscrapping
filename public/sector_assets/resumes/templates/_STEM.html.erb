<%= link_to "Save as PDF", resume_path(@resume ,:format => "pdf"), class: "btn btn-large cyan white-text" , style: "position:fixed; right: 10%"%> 
<div class="header_div"> 

<h5 class="center" style="font-size: 30px"> <%= best_in_place @resume, :name %></h5>

    <div class="row contact_row"> 
        <div class="col s4 center"> 
            <p><%= best_in_place @resume, :address, :place_holder => "<span id='empty'> Enter Your Physical Address </span>" %>  </p>
        </div>
        <div class="col s4 center"> 
            <p><%= best_in_place @resume, :email, :place_holder => "<span id='empty'> Enter Your Email </span>" %> </p>  
        </div>
        <div class="col s4 center"> 
            <p><%= best_in_place @resume, :phone, :place_holder => "<span id='empty'> Enter Your Phone Number </span>" %> </p>  
        </div>
    </div>
</div>
<br> 
<div class="skills_section"> 
<h6 class="education_header">Skills</h6> 
    
    <p> <span style="font-weight:800"> LANGUAGE SKILLS </span><%= best_in_place @resume, :languages, :place_holder => "<span id='empty'> First Language (Conversational), Second Language (Fluent)</span>" %></p>
    <p> <span style="font-weight:800"> COMPUTER SKILLS </span><%= best_in_place @resume, :activities, :place_holder => "<span id='empty'> Technical Skills (Not Incuding Microsoft Word)" %></p>
   
</div>

<div class="education_section"> 
<h6 class="education_header">EDUCATION</h6> 
    <div class="row"> 
        <div class="col s9">
        <p id="academic_position" id="position" style="text-transform: uppercase; font-weight: 800"><%= best_in_place @academic_experience, :position , :place_holder => "<span id='empty'> The School You Are Attending </span>" %></p>
      
        </div>
        
        <div class="col s3">
        <p style="font-weight: 800"><%= best_in_place @academic_experience, :start_date , :place_holder => "<span id='empty'> Expected Graduation & Year </span>" %></p>


        </div>
    </div>
    <p><%= best_in_place @academic_experience, :organization , :place_holder => "<span id='empty'> The Degree & Major You're Getting (ie. Bachelor's of Science in Political Science </span>" %></p>

    <p><%= best_in_place @academic_experience, :detail1 , :place_holder => "<span id='empty'> Relevant Courses You Are Taking .ie Political Theory, Economics </span>" %></p>

</div>

<div class="work_section"> 
<h6 class="education_header">EXPERIENCE</h6> 
   <div class="row" style="margin-botton: -1%"> 
        <div class="col s9">
        <p class="academic_position" id="position" style="text-transform: uppercase; font-weight: 800"><%= best_in_place @work_experience, :position , :place_holder => "<span id='empty'> Your Most Recent Opportunity </span>" %></p>
        </div>
        
        <div class="col s3">
        <p style="font-weight: 800"><%= best_in_place @work_experience, :start_date , :place_holder => "<span id='empty'> Starting Month & Year - Present </span>" %></p>
        </div>
    </div>
    
